  <WorkflowCommandCanExecute>&lt;?xml version="1.0"?&gt;
    &lt;Workflow e="true" isvalid="true" syntaxVersion="11" version="0"&gt;
      &lt;branch e="true" info="Enable or Disable IoT Commands"&gt;
        &lt;if e="true"&gt;
          &lt;conditions e="true" op="And"&gt;
